{
  "version": "1.0.0",
  "manifest_version": 2,
  "name": "Chrome Proxy",
  "permissions": [
    "activeTab",
    "proxy",
    "tabs",
    "debugger",
    "unlimitedStorage",
    "storage",
    "http://*/*",
    "https://*/*",
    "<all_urls>",
    "webRequest",
    "webRequestBlocking"
  ],
  "web_accessible_resources": [
    "/listen_response.js"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": {
    "matches": [
      "http://*/*",
      "https://*/*"
    ],
    "js": ["jquery.js", "content.js"],
    "run_at": "document_start"
  },
  "minimum_chrome_version": "12.0.0"
}
